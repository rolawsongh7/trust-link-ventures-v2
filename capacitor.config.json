{
  "appId": "app.lovable.747d765560614215adc70c8134db2cfc",
  "appName": "Trust Link Ventures",
  "webDir": "dist",
  "server": {
    "allowNavigation": [
      "trustlinkcompany.com",
      "www.trustlinkcompany.com",
      "customer.trustlinkcompany.com",
      "*.supabase.co"
    ]
  },
  "appUrlOpen": {
    "enabled": true,
    "allowedUrls": [
      "https://trustlinkcompany.com/*",
      "https://customer.trustlinkcompany.com/*"
    ],
    "excludedUrls": [
      "**/admin/**",
      "**/dashboard/**",
      "**/crm/**",
      "**/settings/**"
    ]
  },
  "plugins": {
    "Camera": {
      "permissions": [
        "camera",
        "photos"
      ]
    },
    "Geolocation": {
      "permissions": [
        "location"
      ]
    },
    "PushNotifications": {
      "presentationOptions": ["badge", "sound", "alert"]
    },
    "LocalNotifications": {
      "smallIcon": "ic_stat_icon_config_sample",
      "iconColor": "#488AFF",
      "sound": "beep.wav"
    }
  },
  "cordova": {},
  "ios": {
    "scheme": "trust-link-ventures-v2"
  },
  "android": {
    "buildOptions": {
      "keystorePath": "",
      "keystorePassword": "",
      "keystoreAlias": "",
      "keystoreAliasPassword": "",
      "releaseType": "APK"
    }
  }
}